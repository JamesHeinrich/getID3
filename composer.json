    "authors": [{
        "name": "James Heinrich",
        "email": "info@getid3.org",
        "homepage": "https://github.com/JamesHeinrich",
        "role": "Developer"
    },{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require":
    {
        "php": ">=5.3.0"
    },
